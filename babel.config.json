{
  "presets": [
    "@babel/preset-typescript",
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-optional-chaining"
  ],
  "env": {
    "nodejs": {
      "presets": [["@babel/preset-env", {
        "targets": {
          "node": "current"
        },
        "modules": "commonjs"
      }]]
    }
  }
}
